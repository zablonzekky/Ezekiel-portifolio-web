{
  "name": "portfolio-website",
  "version": "1.0.0",
  "description": "Professional Portfolio Website - React Frontend with Supabase Backend",
  "private": true,
  "dependencies": {
    "@dhiwise/component-tagger": "^1.0.9",
    "@tailwindcss/forms": "^0.5.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0",
    "react-scripts": "5.0.1",
    "tailwindcss-animate": "^1.0.7",
    "tailwindcss-elevation": "^2.0.0",
    "tailwindcss-fluid-type": "^2.0.7",
    "web-vitals": "^3.1.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build:css": "npx @dhiwise/component-tagger && npx tailwindcss -i ./src/styles/tailwind.css -o ./src/styles/main.css",
    "watch:css": "npx @dhiwise/component-tagger && npx tailwindcss -i ./src/styles/tailwind.css -o ./src/styles/main.css --watch"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "tailwindcss": "^3.4.17",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/line-clamp": "^0.1.0",
    "@tailwindcss/typography": "^0.5.16"
  }
}